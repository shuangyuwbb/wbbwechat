<view class="tabs">
    <view class="tabs_title">
        <view 
        wx:for="{{tabs}}"
        wx:key="templateTagId"
        class="title_item {{item.isActive ?'active':''}}"
        bind:tap="handleItemTap"
        data-index="{{index}}">{{item.templateTagName}}</view>
    </view>
    <view class="tabs_content">
    <slot ></slot>
    </view>
</view>
